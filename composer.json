{
    "name": "orange-soft/laravel-starter-kit",
    "description": "Laravel starter kit with Inertia, Vue, PrimeVue, and common features",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Orange Soft"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "OrangeSoft\\LaravelStarterKit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OrangeSoft\\LaravelStarterKit\\LaravelStarterKitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
